
# Database Configuration
DATABASE_URL=postgresql://user:password@hostname:5432/database
# For local development with SQLite, leave DATABASE_URL empty

# Session Secret (Auto-generated if not set, but recommended to set in production)
SESSION_SECRET=your-random-secret-key-here

# Server Configuration
NODE_ENV=production
PORT=10000

# AI Features (Optional - for Gemini AI insights)
GEMINI_API_KEY=your-gemini-api-key-here

# Frontend API URL (for mobile app to connect to backend)
VITE_API_URL=https://your-app-name.onrender.com
